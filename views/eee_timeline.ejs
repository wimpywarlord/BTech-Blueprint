    }

    .timeline-panel {
      transform-origin: top right;
      transform-style: preserve-3D;
      transform: rotateY(-90deg);
      transition: all 2s ease-in-out;
    }

    .timeline-inverted .timeline-panel {
      transform-origin: top left;
      transform-style: preserve-3D;
      transition: all 2s ease-in-out;
      transform: rotateY(90deg);
    }

    .timeline>li:before,
    .timeline>li:after {
      content: " ";
      display: table;
    }

    .timeline>li:after {
      clear: both;
    }

    .timeline>li:before,
